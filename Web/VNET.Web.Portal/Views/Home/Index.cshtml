                            <div class="wrp"><a href="#" class="m m2"></a></div>
                            <div class="wrp"><a href="#" class="m m3"></a></div>
                            <div class="wrp"><a href="#" class="m m4"></a></div>
                            <div class="wrp"><a href="#" class="m m5"></a></div>
                            <div class="wrp"><a href="#" class="m m6"></a></div>
                            <div class="wrp"><a href="#" class="m m7"></a></div>
                            <div class="wrp"><a href="#" class="m m8"></a></div>
                            <div class="wrp"><a href="#" class="m m9"></a></div>
                            <div class="wrp"><a href="#" class="m m10"></a></div>
                            <div class="wrp"><a href="#" class="m m11"></a></div>
                            <div class="wrp"><a href="#" class="m m12"></a></div>
                            <div class="wrp"><a href="#" class="m m13"></a></div>
                            <div class="wrp"><a href="#" class="m m14"></a></div>
                            <div class="wrp"><a href="#" class="m m15"></a></div>
                            <div class="fixer"></div>
                        </div>
                    </div>
                    <!--Information Cup-->
                </div>
            </div>
            <!--Left Content-->
            <!--Right Content-->
            <div class="right">
                <div class="bnrContent">
                    <div class="minusMiniSliderHolder">
                        <ul class="slider">
                            <li><img src="~/images/164_ASM_220x304.png" border="0" /></li>
                            <li><img src="~/images/164_CEC_220x304.png" border="0" /></li>
                            <li><img src="~/images/164_KIS_220x304.png" border="0" /></li>
                        </ul>
                        <div class="fixer"></div>
                    </div>
                    <div class="bnrBox">
                        <img src="~/images/Anahtarcilar_Logo_220x304.png" border="0" />
                    </div>
                </div>
                <div class="iframeContent">
                    @Html.Action("Index", "Graffiti")
                </div>
            </div>
            <!--Right Content-->

            <div class="fixer"></div>
            <div class="mbFooter">
                <div class="mbFooterholder"><a href="#" class="wallText"><i></i><span>Duvar Yazısı</span></a><a href="#" class="friends"><i></i><span>Arkadaşlarım</span></a><a href="#" class="education"><i></i><span>Eğitimler</span></a><a href="#" class="comments"><i></i><span>Duyurular</span></a></div>
            </div>
        </div>
